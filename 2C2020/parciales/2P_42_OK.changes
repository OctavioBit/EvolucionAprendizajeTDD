!PlayableCharacterTest methodsFor: 'tests - taking objects' stamp: 'JEA 12/10/2020 21:54:56' prior: 50884992!
test20_canTakePortableObjectsFromNonPortableObject
	| room playableCharacter container subContainer portableObject1 portableObject2 |

	room := self defaultEmptyRoom.
	portableObject1 := self defaultPortableObject.
	portableObject2 := self defaultPortableObject.
	subContainer := NonPortableObject with: portableObject2.
	container := NonPortableObject with: portableObject1 with: subContainer.

	playableCharacter := PlayableCharacter 
		named: 'Luigi' 
		using: self defaultBackpack 
		at: 2@2
		heading: #West 
		inRoom: room.
		
	room add: container at: 1@2.
	
	playableCharacter take.
	
	self assert: (playableCharacter backpackIncludes: portableObject1).
	self assert: (playableCharacter backpackIncludes: portableObject2).
	
	self assert: (playableCharacter backpackIncludes: container) not.
	self assert: (playableCharacter backpackIncludes: subContainer) not.

	self assert: (room hasObject: container).	
	self assert: (room hasObject: subContainer at: 1@2).	

	self assert: (room hasObject: portableObject1) not.
	self assert: (room hasObject: portableObject2) not.! !

