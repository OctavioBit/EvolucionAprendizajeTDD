	|total|
	
	total := 0.
	self checkCartIsNotEmpty: aCart.
	priceList keysAndValuesDo: [: anItem :aItemPrice|  
		(aCart includes: anItem) ifTrue:[total := total + (aItemPrice * (aCart occurrencesOf: anItem))].
