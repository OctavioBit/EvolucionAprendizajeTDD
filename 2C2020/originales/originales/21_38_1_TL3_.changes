!SentenceFinderByPrefix methodsFor: 'initialisation' stamp: 'CJ 10/5/2020 19:22:07'!
initialiseWithStack: anOOStack 
	stack := anOOStack.
	helperStack := OOStack new! !
!SentenceFinderByPrefix methodsFor: 'sentence finding' stamp: 'CJ 10/5/2020 19:21:29'!
find: aPrefix	
	self validatePrefix: aPrefix.
	^self getElementsWithPrefix: aPrefix! !
!SentenceFinderByPrefix methodsFor: 'sentence finding' stamp: 'CJ 10/5/2020 19:38:45'!
getElementsWithPrefix: aPrefix
	| isStringAndBeginsWithPrefix |
	
	isStringAndBeginsWithPrefix := [:aStackElement |
		aStackElement isString and: [aStackElement beginsWith: aPrefix ]
	].
	
	^self filterStackElementsByCondition: isStringAndBeginsWithPrefix! !
!SentenceFinderByPrefix methodsFor: 'helpers' stamp: 'CJ 10/5/2020 19:46:37'!
copy: anElement to: aCollection ifItMeets: aCondition
	(aCondition value: anElement) ifTrue: [
		aCollection addLast: anElement copy
	]! !
!SentenceFinderByPrefix methodsFor: 'helpers' stamp: 'CJ 10/5/2020 19:55:30'!
filterStackElementsByCondition: aCondition
	| matchingElements |
	
	matchingElements := OrderedCollection new.
	
	[stack isEmpty]
		whileFalse: [ | top |
			top := self moveStackTopToHelper.
			self copy: top to: matchingElements ifItMeets: aCondition
		].
	
	self restoreStackFromHelper.
	
	^matchingElements! !
!SentenceFinderByPrefix methodsFor: 'helpers' stamp: 'CJ 10/5/2020 19:54:19'!
moveStackTopToHelper
	helperStack push: stack pop.
	^helperStack top! !
!SentenceFinderByPrefix methodsFor: 'helpers' stamp: 'CJ 10/5/2020 19:55:30'!
restoreStackFromHelper
	[helperStack isEmpty]
		whileFalse: [
			stack push: helperStack pop.
		]! !
!SentenceFinderByPrefix methodsFor: 'helpers' stamp: 'aa 10/5/2020 18:39:16'!
validatePrefix: aPrefix
	aPrefix isEmpty ifTrue: [self error: self class emptyPrefixErrorDescription].
	(aPrefix includesSubString: ' ') ifTrue: [self error: self class prefixWithSpacesErrorDescription]! !

