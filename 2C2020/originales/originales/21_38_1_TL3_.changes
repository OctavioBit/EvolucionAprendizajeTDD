test05PortfolioWithAccountAndPortfolioHasAllTransactions
	
	| outerPortfolio innerPortfolio outerPortfolioAccount innerPortfolioAccount innerPortfolioAccountDeposit outerPortfolioAccountDeposit |
	
	outerPortfolio := Portfolio new.
	innerPortfolio := Portfolio new.
	innerPortfolioAccount := ReceptiveAccount new.
	outerPortfolioAccount := ReceptiveAccount new.
	innerPortfolio add: innerPortfolioAccount.
	outerPortfolio add: innerPortfolio.
	outerPortfolio add: outerPortfolioAccount.
	
	innerPortfolioAccountDeposit := Deposit register: 100 on: innerPortfolioAccount .
	outerPortfolioAccountDeposit := Deposit register: 150 on: outerPortfolioAccount .
	
	self assert: (outerPortfolio hasRegistered: innerPortfolioAccountDeposit).	
	self assert: (outerPortfolio hasRegistered: outerPortfolioAccountDeposit).	! !
