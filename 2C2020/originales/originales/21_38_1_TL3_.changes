step

	manual ifFalse:
	[
		self status handle
	"self enteringCurve
		ifTrue: [motor decelerate.
			self inFrontOfAnObstacle
				ifTrue: [self pushAllBreaks]
				ifFalse: [self freeAllBreaks]
			]
		ifFalse:[
			self inFrontOfAnObstacle
				ifTrue: [motor decelerate. self pushAllBreaks]
				ifFalse: [motor accelerate. self freeAllBreaks]
			]"
	]
	ifTrue:
	[
		self inFrontOfAnObstacle
			ifTrue: [self pushAllBreaks]
	]
! !

