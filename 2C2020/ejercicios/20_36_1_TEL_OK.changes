test21OCantMoveTwiceInARow
	"x no puede mover dos veces seguidas"
	| aGame |
	aGame := self createGameInMovePhase.
	
	aGame xMoves: (2@0) to: (1@0).
	aGame oMoves: (2@0) to: (1@0).

	self should: [aGame oMoves: (1@0) to: (2@0).] raise: Error withMessageText: TerniLapilliGame wrongTurnErrorMessage.
! !
