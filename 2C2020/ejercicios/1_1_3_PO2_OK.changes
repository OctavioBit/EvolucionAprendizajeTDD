reportAccount: anAccount formatWith: formatForAccount evaluating: aBlock
		
	treeStructure add: (self leadingTabs, formatForAccount).
	
	numberOfLeadingTabs := numberOfLeadingTabs + 1.
	
	aBlock value.
	
	numberOfLeadingTabs := numberOfLeadingTabs - 1.! !

