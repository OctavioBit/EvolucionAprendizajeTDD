	| itemsInCart output cart |
	cart := carts at: cartID ifAbsent: [^OrderedCollection with: 1 with: 'Invalid Cart ID.'].
	itemsInCart := cart listItems.
	output := OrderedCollection with: 0.
	itemsInCart do: [ :item | 
			output addLast: item.
			output addLast: (cart occurrencesOf: item).
		 ].
	^output.! !

