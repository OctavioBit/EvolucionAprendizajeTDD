test30OCantPutAfterXWon
	"O no puede poner despues de que X gano"
	| aGame |
	aGame := TerniLapilliGame new.
	
	aGame putXAt: 0@1.
	aGame putOAt: 0@2.
	aGame putXAt: 1@1.
	aGame putOAt: 1@2.
