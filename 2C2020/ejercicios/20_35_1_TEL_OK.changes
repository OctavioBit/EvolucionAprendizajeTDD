test25OCantMoveOnTheirFirstTurn
	"o no puede mover en su primer turno"
	| aGame |
	aGame := TerniLapilliGame new.
	
	aGame putXAt: 0@0.

	self should: [aGame moveOFrom: (0@0) to: (1@0).] raise: Error withMessageText: TerniLapilliGame cannotMoveErrorMessage.

