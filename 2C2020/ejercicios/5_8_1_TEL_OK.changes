	| aTerniLapilliGame |
	
	aTerniLapilliGame := TerniLapilli new.
	
	aTerniLapilliGame placeXAt: 1@2.
	aTerniLapilliGame placeOAt: 1@1.
	aTerniLapilliGame placeXAt: 2@1.
	aTerniLapilliGame placeOAt: 1@3.
	aTerniLapilliGame placeXAt: 2@2.
	aTerniLapilliGame placeOAt: 3@1.
	
	aTerniLapilliGame moveXFrom: 2@2 to: 3@3. 
	aTerniLapilliGame moveOFrom: 1@3 to: 2@3. 

	self assert: (aTerniLapilliGame isOccupiedByO: 2@3).
	self deny: (aTerniLapilliGame isOccupiedByO: 1@3).! !

