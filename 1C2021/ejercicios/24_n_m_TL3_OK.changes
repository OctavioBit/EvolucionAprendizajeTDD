	cart := self getCart: aShoppingCartId. 
	
	clientId := self getClientIdOf: aShoppingCartId.

	(cart entries) ifEmpty: [ self error: Cashier cantCheckoutEmptyCartErrorDescription ].
	
	receiptBooks at: clientId put: cart entries size.! !
