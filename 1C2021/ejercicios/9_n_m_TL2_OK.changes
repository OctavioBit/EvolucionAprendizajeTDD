!AccountSummary methodsFor: 'as yet unclassified' stamp: 'TPL 6/5/2021 13:25:45'!
                           getReportForAsset: anAsset
	|report|
	anAsset transactions ifEmpty: [^ OrderedCollection with: 'Balance: 0'].
	report _ OrderedCollection new.
	anAsset transactions do: [:aTransaction | aTransaction addInformationTo: report].
	report add: 'Balance: ', (anAsset balance) asString.
	^report! !
