!methodRemoval: AccountSummary #getReportForPortfolio: stamp: 'TPL 6/5/2021 13:34:39'!
getReportForPortfolio: aPortfolio
	| report|
	aPortfolio transactions ifEmpty: [^ OrderedCollection with: 'Balance: 0'].
	report _ OrderedCollection new.
	aPortfolio transactions do: [:aTransaction | aTransaction addInformationTo: report].
	report add: 'Balance: ', (aPortfolio balance) asString.
	^report!

!methodRemoval: AccountSummary #getReportForAccount: stamp: 'TPL 6/5/2021 13:34:41'!
getReportForAccount: account
	|report|
	account transactions ifEmpty: [^ OrderedCollection with: 'Balance: 0'].
	report _ OrderedCollection new.
	account transactions do: [:aTransaction | aTransaction addInformationTo: report].
	report add: 'Balance: ', (account balance) asString.
	^report!

