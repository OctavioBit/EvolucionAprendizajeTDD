	tankRoverRobot addWeapon: turboLaserWeapon .
	tankRoverRobot addWeapon: clawWeapon.

	self assert: turboLaserWeapon weight + clawWeapon weight equals: tankRoverRobot totalWeaponWeight.
	
	

