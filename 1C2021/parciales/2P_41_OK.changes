!RondaDeTruco methodsFor: 'as yet unclassified' stamp: 'ENC 7/5/2021 21:21:17' prior: 50997651!
tiraMano: unaCartaDeLaMano
	
	self assertValidCard: unaCartaDeLaMano for: mano.
	self assertValidOrderInMatchForMano.
	self assertValidMatch.
	
	mano remove: unaCartaDeLaMano.
	cartasDeLaMesa add: unaCartaDeLaMano.
	
	"cÛdigo repetido entre tiraMano y tiraPie..."
	(cartasDeLaMesa size = 2) ifTrue: [
		(unaCartaDeLaMano empardaCon: cartasDeLaMesa first) ifTrue: [
			self enfrentamientoEmpardado.
		] ifFalse: [
			(unaCartaDeLaMano mataA: cartasDeLaMesa first) ifTrue: [
				turnoMano := true.	
				puntosMano := puntosMano + 1.
			] ifFalse: [
				turnoMano := false.
				puntosPie := puntosPie + 1.
			]
		].

		cartasDeLaMesa := OrderedCollection new.
	] ifFalse: [
		turnoMano := false.
	]! !
!RondaDeTruco methodsFor: 'as yet unclassified' stamp: 'ENC 7/5/2021 21:21:17' prior: 50997590!
tiraPie: unaCartaDelPie
	
	self assertValidCard: unaCartaDelPie for: pie. 
	self assertValidOrderInMatchForPie.
	self assertValidMatch.

	pie remove: unaCartaDelPie.
	cartasDeLaMesa add: unaCartaDelPie.
	
	(cartasDeLaMesa size = 2) ifTrue: [
		(unaCartaDelPie empardaCon: cartasDeLaMesa first) ifTrue: [
			self enfrentamientoEmpardado.
		] ifFalse: [
			(unaCartaDelPie mataA: cartasDeLaMesa first) ifTrue: [
				turnoMano := false.	
				puntosPie := puntosPie + 1.
			] ifFalse: [
				turnoMano := true.
				puntosMano := puntosMano + 1.
			]
		].

		cartasDeLaMesa := OrderedCollection new.
	] ifFalse: [
		turnoMano := true.
	]
! !

