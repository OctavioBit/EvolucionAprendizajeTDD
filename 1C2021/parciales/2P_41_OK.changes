!RondaDeTruco methodsFor: 'playing' stamp: 'ENC 7/5/2021 19:45:34' prior: 50996542!
tiraPie: unaCartaDelPie
	
	self assertValidCard: unaCartaDelPie for: pie. 
	self assertValidOrderInMatch: turnoPie.
	
	pie remove: unaCartaDelPie.
	cartas add: unaCartaDelPie.
	
	(unaCartaDelPie empardaCon: cartas first) ifTrue: [ "cartas va a ser el historial"
		turnoPie := false.
		turnoMano := true.
		puntosMano := puntosMano + 1.
		puntosPie := puntosPie + 1.
	] ifFalse: [
		(unaCartaDelPie mataA: cartas first) ifTrue: [
			turnoPie := true.
			turnoMano := false.	
			puntosPie := puntosPie + 1.
		] ifFalse: [
			turnoPie := false.
			turnoMano := true.
			puntosMano := puntosMano + 1.
		]
	].

	cartas := OrderedCollection new.
	
! !
