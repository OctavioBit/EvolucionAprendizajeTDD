	cartasJugadas add: unaCartaDeTruco.
	
	(cartasJugadas size = 4) 
		ifTrue: [	(self quienGanoElSegundoEnfrentamiento = #Pie) 
			ifTrue: [turnosState := TurnoDelPieState for: self]
