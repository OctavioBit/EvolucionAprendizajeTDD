	|childsPortfolio raizParents parentsPortfolio|	
	raizParents := OrderedCollection new.
	self isRaiz ifTrue: [
		childsPortfolio:= aPortfolio getChilds .
		childsPortfolio isEmpty 
			ifTrue: [
			aPortfolio addParent: self.
			childrens addLast: aPortfolio ]
			ifFalse: [
				childsPortfolio detect: [:aChild | (childrens includes: aChild) = 				false ] ifNone: 	[^Error signal: self class describeErrorCantHaveRepeted].
		].
	] ifFalse: [
		parents do: [:aParent | 
			aParent isRaiz ifTrue: 
				[
					aParent compareWithAllRaices: aPortfolio .
					"aParent addPortfolio: aPortfolio" ] 
			"me esta faltando el false"
		].
		aPortfolio addParent: self.
		childrens addLast: aPortfolio 
	].
	 ! !
