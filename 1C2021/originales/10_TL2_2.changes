!Robot methodsFor: 'weapons' stamp: 'ms 5/3/2021 17:28:37' prior: 50964331!
totalWeaponWeight
	weapons isNil ifTrue: [ weapons := OrderedCollection new ].
	
	weapons sum: [:weapon | weapon weight.] ifEmpty: [0].
	"| total index |

	weapons isNil ifTrue: [ weapons := OrderedCollection new ].

	total := 0 * kilogram.
	index := 1.
	
	[index <= weapons size] whileTrue: [ | weapon |
		weapon := (weapons at: index).
		total := total + weapon weight.
		index := index + 1.	
	].

	^ total.	"! !

