!methodRemoval: Backpack #total: stamp: 'ms 5/2/2021 15:41:38'!
total: aWeight
	| total index |

	total := 0.
	index := 1.
	
	[index <= container size] whileTrue: [ | portableObject |
		portableObject := (container at: index).
		
		(aWeight = 'Weight') ifTrue: [total := total + portableObject weight.] ifFalse: [(portableObject doesItAddPoints) ifTrue: [ total := total +  portableObject points ].].
		
		index := index + 1.	
	].

	^ total.!

