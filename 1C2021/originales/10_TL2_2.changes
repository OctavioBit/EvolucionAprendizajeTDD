|transfer accountSummary accountSummaryReporter|
	transfer := Transfer new.
	transfer transferFromTheAccount: anAccount1 toAccount: anAccount2 withAmount: 10.

	accountSummaryReporter := AccountSummary for: anAccount1.
	accountSummary := accountSummaryReporter accountSummary.
	
	self assert: (accountSummary at: 1) equals:  anTransferAction ,  'Salida por transferencia de 10' printString.
	self assert: (accountSummary at: 2) equals: 'Balance =', aBalance printString .
	self assert: (accountSummary size) equals: 2.! !
