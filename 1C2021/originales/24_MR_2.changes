!methodRemoval: MarsRover #directionToTheLeftOf: stamp: 'AW 5/16/2021 18:37:35'!
directionToTheLeftOf: aDirection 
	
	(aDirection = #S) ifTrue: [		^#E.	].
	(aDirection = #E) ifTrue: [		^#N.].
	(aDirection = #N) ifTrue: [		^#W.	].
	(aDirection = #W) ifTrue: [		^#S.	].!

!methodRemoval: MarsRover #directionToTheRightOf: stamp: 'AW 5/16/2021 18:37:37'!
directionToTheRightOf: aDirection

	(aDirection = #S) ifTrue: [		^#W.	].
	(aDirection = #E) ifTrue: [		^#S.].
	(aDirection = #N) ifTrue: [		^#E.	].
	(aDirection = #W) ifTrue: [		^#N.	].!

!methodRemoval: MarsRover #getBackwardVectorFor: stamp: 'AW 5/16/2021 18:37:39'!
getBackwardVectorFor: aDirection 
	^ (self getForwardVectorFor: aDirection ) negated.!

!methodRemoval: MarsRover #getForwardVectorFor: stamp: 'AW 5/16/2021 18:37:41'!
getForwardVectorFor: aDirection
	
	(aDirection = #N) ifTrue: [
		^ 0 @ 1.		
	].
	(aDirection = #E) ifTrue: [
		^ 1 @ 0.		
	].
	(aDirection = #W) ifTrue: [
		^ -1 @ 0.		
	].
	(aDirection = #S) ifTrue: [
		^ 0 @	-1.		
	].
!

