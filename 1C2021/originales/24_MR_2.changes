!methodRemoval: MarsRover #processInstruction:onInvalidInstruction: stamp: 'nz 5/16/2021 23:06:45'!
processInstruction: aInstruction onInvalidInstruction: anErrorBlock
	(aInstruction = $f) ifTrue: [
		^self goForward.
	].
	(aInstruction = $b) ifTrue: [
		^self goBackward.
	].
	(aInstruction = $l) ifTrue: [
		^self turnLeft.		
	].
	(aInstruction = $r) ifTrue: [
		^self turnRight.
	].
	
	^anErrorBlock value.!

