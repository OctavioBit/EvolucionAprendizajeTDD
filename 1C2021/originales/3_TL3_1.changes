findByPrefix: aPrefix

	| matches auxiliaryStack |

	aPrefix isEmpty ifTrue: [ self error: self class prefixCantBeEmptyErrorDescription ].
	(aPrefix includesSubString: ' ') ifTrue: [self error: self class prefixCantHaveEmptySpaces] .

	auxiliaryStack := OOStack new.
	
	matches := 	(self movePiecesFrom: stack to: auxiliaryStack) select:[:anElement | anElement beginsWith: aPrefix].
	
	self movePiecesFrom: auxiliaryStack  to: stack.	
	
	^ matches.	! !

