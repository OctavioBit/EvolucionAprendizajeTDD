!Room methodsFor: 'contents' stamp: 'FG 1/7/2021 19:34:29' prior: 50930766!
moveFrom: aSourcePosition to: aTargetPosition

	| toMove |
	
	toMove := self at: aSourcePosition.
	((self isEmptyAt: aTargetPosition) not and:[(self at: aTargetPosition) isKindOf: Door]) 
		ifTrue: [
			(self at: aTargetPosition) move: (self at: aSourcePosition).	
		] 
		ifFalse: [
			self at: aTargetPosition put: toMove.
			contents removeKey: aSourcePosition.
		].
	! !
