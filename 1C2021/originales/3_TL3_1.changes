	room2 := Room ofWidth: 5 andHeight: 5.

	self
		should: [ Door placedIn: room1 inPosition: -1@1  connectedTo: room2 inPosition: 1@1 ]
		raise: Error
		withMessageText: Room invalidPositionErrorDescription.
		
	self assert: (room1 isEmptyAt: -1@1).
	self assert: (room2 isEmptyAt: 1@1).
		
	self
		should: [ Door placedIn: 1.2@1 of: room1  andIn: 1@1  of: room2  ]
		raise: Error
		withMessageText: Room invalidPositionErrorDescription.
		
	self assert: (room1 isEmptyAt: 1.2@1).
	self assert: (room2 isEmptyAt: 1@1).	
				
	self
		should: [ Door placedIn: 1@1 of: room1  andIn: -1@1  of: room2  ]
		raise: Error
		withMessageText: Room invalidPositionErrorDescription.

	self assert: (room1 isEmptyAt: 1@1).
	self assert: (room2 isEmptyAt: -1@1).
	
	self
		should: [ Door placedIn: 1@2 of: room1  andIn: 1.5@1  of: room2  ]
