	(aCommand = 'b') ifTrue:[ position := position - direction ].
	
	(aCommand = 'l') ifTrue:[ 
			direction = (-1@0) ifTrue: [direction := 0@-1]
							ifFalse: [
								direction = (0@1) ifTrue: [direction := -1@0]
									ifFalse:[
										direction = (0@-1) ifTrue:[ direction := (1@0) ]
											ifFalse: [ 
												direction = (1@0) ifTrue: [ direction := (0@1) ]
											]
									]
							] 
		].
	
		(aCommand = 'r') ifTrue:[ 
			direction = (0@1) ifTrue: [direction := 1@0]
							ifFalse: [
								direction = (1@0) ifTrue: [direction := 0@-1]
									ifFalse:[
										direction = (0@-1) ifTrue:[ direction := (-1@0) ]
											ifFalse: [ 
												direction = (-1@0) ifTrue: [ direction := (0@1) ]
											]
									]
							] 
		].! !

