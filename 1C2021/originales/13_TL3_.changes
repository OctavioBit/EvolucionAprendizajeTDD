!WindowTest methodsFor: 'directonAndPositionWindow' stamp: 'sc 6/5/2021 22:44:20'!
test05SiHayDosVentanasE
	|rover window|
	rover _ MarsRover at: (0@0) heading: #North.
	window _ RoverWindow newWith: rover.
	rover process: 'l'.
	self assert: window position equals: (0@0).
	self assert: window direction equals: (#West).! !
!WindowTest methodsFor: 'directonAndPositionWindow' stamp: 'sc 6/5/2021 22:46:36'!
test5SePuedenTenerDosLogsFuncionandoSobreUnMismoRover
	|rover aWindow anotherWindow|
	rover _ MarsRover at: (0@0) heading: #North.
	aWindow _ RoverWindow newWith: rover.
	anotherWindow _ RoverWindow newWith: rover.
	rover process: 'llbr'.
	self assert: aWindow position equals: (0@0).
	self assert: aWindow direction equals: (#West).
	
	self assert: anotherWindow position equals: (0@0).
	self assert: anotherWindow direction equals: (#West).
! !

